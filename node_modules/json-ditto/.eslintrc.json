{
  "extends": [
    "eslint:recommended"
  ],
  "plugins": [
  ],
  "parserOptions": {
    "ecmaVersion": 9
  },
  "env": {
    "es6": true,
    "node": true,
    "mocha": true
  },
  "rules": {
    "no-mixed-spaces-and-tabs": 0,
    "no-unused-vars": 0,
    "no-extra-boolean-cast": 0,
    "no-useless-escape": 0,
    "no-console": 0,
    "no-extra-semi": 0,
    "no-irregular-whitespace": 0,
    "no-inner-declarations": 0,
    "no-unsafe-finally": 0
  }
}